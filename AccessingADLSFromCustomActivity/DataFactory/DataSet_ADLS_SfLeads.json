{
  "$schema": "http://datafactories.schema.management.azure.com/schemas/2015-08-01/Microsoft.DataFactory.Table.json",
  "name": "DataSet_ADLS_SfLeads",
  "properties": {
    "published": false,
    "type": "AzureDataLakeStore",
    "linkedServiceName": "LinkedService_ADLS_SampleADL",
    "typeProperties": {
      "folderPath": "RawData/Salesforce/Lead/{Year}/{Month}/{Day}/",
      "format": {
        "type": "TextFormat",
        "columnDelimiter": ",",
        "quoteChar": ""
      },
      "partitionedBy": [
        {
          "name": "Year",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "yyyy"
          }
        },
        {
          "name": "Month",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "MM"
          }
        },
        {
          "name": "Day",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "dd"
          }
        }
      ]
    },
    "availability": {
      "frequency": "Day",
      "interval": 1
    }
  }
}